.containner {
    display: flex;
    flex-direction: column;
    align-items: center;
}

.errorTip {
    width: 100%;
    height: 380rpx;
    display: flex;
    flex-direction: row;
    position: relative;
}

.errorTip .tip {
    position: absolute;
    float: right;
    top: 100rpx;
    right: 80rpx;
    z-index: 99;
}

.errorTip image {
    position: absolute;
    float: right;
    right: 0rpx;
    width: 550rpx;
    height: 380rpx;
}

.errorImg {
    width: 100%;
}

.errorImg image {
    margin-left: 20rpx;
    width: 600rpx;
    height: 600rpx;
}

.back{
    text-decoration-line: underline;
    color: blue;
}
