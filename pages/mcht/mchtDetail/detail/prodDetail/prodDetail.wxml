<view class="page">
  <include src="../../../common/step-nav.wxml" />
  <view class="page__bd">
    <view class="weui-cells weui-cells_after-title">
      <view class="weui-cell weui-cell_input">
        <view class="weui-cell__hd">
          <view class="weui-label">产品名称</view>
        </view>
        <view class="weui-cell__bd__d">
          <text class="weui-text" name="productName">{{productName}}</text>
        </view>
      </view>
      <view class="weui-cell weui-cell_input">
        <view class="weui-cell__hd">
          <view class="weui-label">借记卡手续费(%)</view>
        </view>
        <view class="weui-cell__bd__d">
          <text class="weui-text" name="jiejiFee">{{jiejiFee}}</text>
        </view>
      </view>
      <view class="weui-cell weui-cell_input">
        <view class="weui-cell__hd">
          <view class="weui-label">借记卡费率封顶(元)</view>
        </view>
        <view class="weui-cell__bd__d">
          <text class="weui-text" name="jiejiTop">{{jiejiTop}}</text>
        </view>
      </view>
      <view class="weui-cell weui-cell_input">
        <view class="weui-cell__hd">
          <view class="weui-label">贷记卡手续费(%)</view>
        </view>
        <view class="weui-cell__bd__d">
          <text class="weui-text" name="daijiFee">{{daijiFee}}</text>
        </view>
      </view>
      <view class="weui-cell weui-cell_input">
        <view class="weui-cell__hd">
          <view class="weui-label">贷记卡费率封顶(元)</view>
        </view>
        <view class="weui-cell__bd__d">
          <text class="weui-text" name="daijiTop">{{daijiTop}}</text>
        </view>
      </view>
      <view class="weui-cell weui-cell_input">
        <view class="weui-cell__hd">
          <view class="weui-label">单笔最小限额(元)</view>
        </view>
        <view class="weui-cell__bd__d">
          <text class="weui-text" name="limitMin">{{limitMin}}</text>
        </view>
      </view>
      <view class="weui-cell weui-cell_input">
        <view class="weui-cell__hd">
          <view class="weui-label">单笔最大限额(元)</view>
        </view>
        <view class="weui-cell__bd__d">
          <text class="weui-text" name="limitOne">{{limitOne}}</text>
        </view>
      </view>
      <view class="weui-cell weui-cell_input">
        <view class="weui-cell__hd">
          <view class="weui-label">单日累计限额(元)</view>
        </view>
        <view class="weui-cell__bd__d">
            <text class="weui-text" name="limitDay">{{limitDay}}</text>
        </view>
      </view>
      <view class="weui-cell weui-cell_input">
        <view class="weui-cell__hd">
          <view class="weui-label">月累计限额(元)</view>
        </view>
        <view class="weui-cell__bd__d">
            <text class="weui-text" name="limitMonth">{{limitMonth}}</text>
        </view>
      </view>
      <view class="weui-cell weui-cell_input">
        <view class="weui-cell__hd">
          <view class="weui-label">年累计限额(元)</view>
        </view>
        <view class="weui-cell__bd__d">
            <text class="weui-text" name="limitYear">{{limitYear}}</text>
        </view>
      </view>
    </view>
  </view>
</view>